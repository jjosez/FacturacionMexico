<?xml version="1.0" encoding="UTF-8"?>
<!--
    Document   : cfdisclientes.xml
    Author     : Juan JosÃ© Prieto Dzul 
    Description:
        Structure for the cfdisclientes table.
-->
<table>
    <column>
        <name>cfdiglobal</name>
        <type>boolean</type>
    </column>
    <column>
        <name>codcliente</name>
        <type>character varying(10)</type>
    </column>
    <column>
        <name>coddivisa</name>
        <type>character varying(3)</type>
        <null>NO</null>
    </column>
    <column>
        <name>created_at</name>
        <type>timestamp</type>
    </column>
    <column>
        <name>emisor_nombre</name>
        <type>character varying(100)</type>
    </column>
    <column>
        <name>emisor_rfc</name>
        <type>character varying(13)</type>
    </column>
    <column>
        <name>estado</name>
        <type>character varying(10)</type>
        <null>NO</null>
    </column>
    <column>
        <name>fecha_emision</name>
        <type>timestamp</type>
        <null>NO</null>
    </column>
    <column>
        <name>fecha_timbrado</name>
        <type>timestamp</type>
        <null>NO</null>
    </column>
    <column>
        <name>filename</name>
        <type>character varying(100)</type>
        <null>YES</null>
        <default>NULL</default>
    </column>
    <column>
        <name>folio</name>
        <type>character varying(40)</type>
        <null>NO</null>
    </column>
    <column>
        <name>forma_pago</name>
        <type>character varying(10)</type>
        <null>NO</null>
    </column>
    <column>
        <name>id</name>
        <type>serial</type>
        <null>NO</null>
    </column>
    <column>
        <name>idempresa</name>
        <type>integer</type>
    </column>
    <column>
        <name>idfactura</name>
        <type>integer</type>
        <null>NO</null>
    </column>        
    <column>
        <name>last_nick</name>
        <type>character varying(50)</type>
    </column>
    <column>
        <name>metodo_pago</name>
        <type>character varying(10)</type>
        <null>NO</null>
    </column>
    <column>
        <name>nick</name>
        <type>character varying(50)</type>
    </column>
    <column>
        <name>receptor_nombre</name>
        <type>character varying(100)</type>
        <null>NO</null>
    </column>
    <column>
        <name>receptor_rfc</name>
        <type>character varying(13)</type>
        <null>NO</null>
    </column>
    <column>
        <name>serie</name>
        <type>character varying(25)</type>
        <null>NO</null>
    </column>
    <column>
        <name>tipo</name>
        <type>character varying(1)</type>
        <null>NO</null>
    </column>
    <column>
        <name>total</name>
        <type>double precision</type>
        <null>YES</null>
    </column>
    <column>
        <name>updated_at</name>
        <type>timestamp</type>
    </column>
    <column>
        <name>uuid</name>
        <type>character varying(36)</type>
        <null>NO</null>
    </column>
    <column>
        <name>version</name>
        <type>character varying(4)</type>
        <null>NO</null>
    </column>
    <constraint>
        <name>cfdis_clientes_pkey</name>
        <type>PRIMARY KEY (id)</type>
    </constraint>
    <constraint>
        <name>uniq_uuid_cfdis_clientes</name>
        <type>UNIQUE (uuid,idempresa)</type>
    </constraint>
    <constraint>
        <name>ca_cfdis_clientes_empresas</name>
        <type>FOREIGN KEY (idempresa) REFERENCES empresas (idempresa) ON DELETE RESTRICT ON UPDATE CASCADE</type>
    </constraint>
    <constraint>
        <name>ca_cfdis_proveedores_codcliente</name>
        <type>FOREIGN KEY (codcliente) REFERENCES clientes (codcliente) ON DELETE RESTRICT ON UPDATE CASCADE
        </type>
    </constraint>
    <constraint>
        <name>ca_cfdis_clientes_facturascli</name>
        <type>FOREIGN KEY (idfactura) REFERENCES facturascli (idfactura) ON DELETE RESTRICT ON UPDATE CASCADE</type>
    </constraint>
    <constraint>
        <name>ca_cfdis_clientes_nick</name>
        <type>FOREIGN KEY (nick) REFERENCES users (nick) ON DELETE SET NULL ON UPDATE CASCADE</type>
    </constraint>
    <constraint>
        <name>ca_cfdis_clientes_last_nick</name>
        <type>FOREIGN KEY (last_nick) REFERENCES users (nick) ON DELETE SET NULL ON UPDATE CASCADE</type>
    </constraint>
</table>
